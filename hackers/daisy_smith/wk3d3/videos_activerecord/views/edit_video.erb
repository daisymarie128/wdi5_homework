<div class="formholder">
  <h1>EDIT VIDEO</h1>
<form action="/videos/<%= @video.id %>" method="post">
  <label for="title">Title: </label>
  <br>
  <input type="text" id="title" name="title"value="<%= @video.title %>">>
  <br>
  <br>
    <label for="description">Description: </label>
  <input type="text" id="description" name="description"value="<%= @video.description %>">>
  <br>
  <br>

  <label for="url">Url: </label>
  <br>
  <input type="text" id="url" name="url"value="<%= @video.url %>">>
  <br>
  <label for="genre">Genre: </label>
  <br>
  <select name="genre" id="genre"value="<%= @video.genre %>">>
    <% %w{Funny Tutorial Design Game News}.each do |genre| %>
      <option><%= genre %></option>
    <% end %>
  </select>
  <br>
  <br>
  <button>Edit Video</button>
</form>
</div><!--//////close form holder-->